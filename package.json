{
  "name": "cocolis-js",
  "version": "0.0.3",
  "description": "Javascript SDK to use Cocolis API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cocolis-1/cocolis-js.git"
  },
  "keywords": [
    "Cocolis",
    "SDK"
  ],
  "author": "Cocolis <it@cocolis.fr> (https://www.cocolis.fr/)",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/Cocolis-1/cocolis-js/issues"
  },
  "homepage": "https://github.com/Cocolis-1/cocolis-js#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/querystringify": "^2.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "nock": "^13.1.1",
    "np": "^7.5.0",
    "querystringify": "^2.2.0"
  }
}
